{
  "buffer_line_height": "comfortable",
  "colorize_brackets": true,
  "agent_servers": {
    "opencode": {
      "type": "extension",
      "favorite_models": [
        "openai/gpt-5.2-codex"
      ],
      "default_config_options": {},
      "favorite_config_option_values": {}
    },
    "claude": {
      "default_mode": "plan",
    },
  },
  "agent_ui_font_size": 14.0,
  "gutter": {
    "line_numbers": false,
  },
  "buffer_font_weight": 400.0,
  "agent_buffer_font_size": 12,
  "agent": {
    "enable_feedback": false,
    "use_modifier_to_send": false,
    "commit_message_model": {
      "provider": "anthropic",
      "model": "claude-haiku-4-5-latest",
    },
    "inline_assistant_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-5",
    },
    "always_allow_tool_actions": true,
    "default_model": {
      "model": "claude-opus-4-5-latest",
      "provider": "anthropic",
    },
    "default_profile": "write",
    "dock": "right",
    "notify_when_agent_waiting": "primary_screen",
    "profiles": {
      "minimal": {
        "name": "Minimal",
        "tools": {},
        "enable_all_context_servers": false,
        "context_servers": {
          "svelte": {
            "tools": {
              "svelte-autofixer": true,
              "playground-link": true,
              "list-sections": true,
              "get-documentation": true,
            },
          },
        },
      },
      "ask": {
        "default_model": {
          "provider": "anthropic",
          "model": "claude-sonnet-4-5-latest",
        },
        "context_servers": {
          "svelte": {
            "tools": {
              "svelte-autofixer": true,
              "playground-link": true,
              "list-sections": true,
              "get-documentation": true,
            },
          },
        },
        "enable_all_context_servers": false,
        "name": "Ask",
        "tools": {
          "contents": true,
          "diagnostics": true,
          "fetch": true,
          "find_path": true,
          "grep": true,
          "list_directory": true,
          "now": true,
          "read_file": true,
          "thinking": true,
          "web_search": true,
        },
      },
      "write": {
        "default_model": {
          "provider": "anthropic",
          "model": "claude-sonnet-4-5-latest",
        },
        "context_servers": {
          "svelte": {
            "tools": {
              "svelte-autofixer": true,
              "list-sections": true,
              "get-documentation": true,
            },
          },
        },
        "enable_all_context_servers": true,
        "name": "Write",
        "tools": {
          "batch_tool": true,
          "code_actions": true,
          "code_symbols": true,
          "contents": true,
          "copy_path": false,
          "create_file": true,
          "delete_path": false,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "find_path": true,
          "grep": true,
          "list_directory": true,
          "move_path": false,
          "now": true,
          "read_file": true,
          "rename": true,
          "symbol_info": true,
          "terminal": true,
          "thinking": true,
          "web_search": true,
        },
      },
    },
  },
  "auto_install_extensions": {
    "elixir": true,
    "fish": true,
    "html": true,
    "toml": true,
  },
  "buffer_font_family": "TX-02",
  "buffer_font_size": 12,
  "collaboration_panel": {
    "button": false,
  },
  "context_servers": {
    "postgres": {
      "enabled": true,
      "settings": {},
    },
  },
  "current_line_highlight": "gutter",
  "cursor_blink": false,
  "edit_predictions": {
    "disabled_globs": [".env", "**/journal"],
    "mode": "subtle",
  },
  "experimental.theme_overrides": {
    "players": [
      {
        "selection": "#99FFE43d",
      },
    ],
  },
  "features": {
    "edit_prediction_provider": "zed",
  },
  "file_types": {
    "CSS": ["scss"],
    "Shell Script": [".envrc", ".env"],
  },
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
  "icon_theme": "Colored Zed Icons Theme Dark",
  "inlay_hints": {
    "enabled": false,
  },
  "journal": {
    "hour_format": "hour24",
    "path": "~/Notes",
  },
  "language_models": {
    "lmstudio": {
      "available_models": [
        {
          "display_name": "qwen3-coder-30b",
          "max_tokens": 128000,
          "name": "qwen/qwen3-coder-30b",
          "supports_images": false,
          "supports_tool_calls": true,
        },
      ],
    },
  },
  "languages": {
    "Elixir": {
      "language_servers": [
        "expert",
        "!elixir-ls",
        "!next-ls",
        "!lexical",
        "tailwindcss-language-server",
      ],
    },
    "HEEX": {
      "language_servers": [
        "expert",
        "!elixir-ls",
        "!next-ls",
        "!lexical",
        "tailwindcss-language-server",
      ],
    },
    "HTML": {
      "hard_tabs": false,
      "tab_size": 2,
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true,
      },
      "hard_tabs": false,
      "tab_size": 2,
    },
    "Markdown": {
      "show_edit_predictions": false,
    },
    "Svelte": {
      "hard_tabs": false,
      "tab_size": 2,
    },
    "TypeScript": {
      "hard_tabs": false,
      "tab_size": 2,
    },
  },
  "lsp": {
    "vscode-css-language-server": {
      "settings": {
        "css": {
          "validate": false,
        },
      },
    },
    "expert": {
      "binary": {
        "path": "/Users/petar/.local/bin/expert_darwin_arm64",
        "arguments": ["--stdio"],
      },
    },
  },
  "middle_click_paste": true,
  "preferred_line_length": 100,
  "project_panel": {
    "hide_root": false,
    "dock": "left",
  },
  "relative_line_numbers": "enabled",
  "scrollbar": {
    "show": "never",
  },
  "show_completions_on_input": true,
  "show_edit_predictions": true,
  "soft_wrap": "editor_width",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "terminal": {
    "scrollbar": {
      "show": "auto",
    },
    "blinking": "off",
    "copy_on_select": true,
    "dock": "bottom",
    "env": {
      "TERM_PROGRAM": "zed",
    },
    "font_family": "TX-02",
    "font_size": 12,
    "option_as_meta": true,
    "shell": {
      "with_arguments": {
        "title_override": "zsh",
        "args": ["-l", "-i"],
        "program": "/opt/homebrew/bin/zsh",
      },
    },
    "toolbar": {
      "breadcrumbs": false,
    },
  },
  "theme": "Zed Dark",
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
  },
  "ui_font_family": "iA Writer Duospace",
  "ui_font_size": 13.0,
  "use_autoclose": true,
  "vertical_scroll_margin": 5,
  "vim": {
    "toggle_relative_line_numbers": true,
  },
  "vim_mode": true,
}
