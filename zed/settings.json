{
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "model": "gpt-5",
      "provider": "openai"
    },
    "default_profile": "write",
    "dock": "right",
    "notify_when_agent_waiting": "never",
    "profiles": {
      "ask": {
        "enable_all_context_servers": false,
        "name": "Ask",
        "tools": {
          "contents": true,
          "diagnostics": true,
          "fetch": true,
          "find_path": true,
          "grep": true,
          "list_directory": false,
          "now": true,
          "read_file": true,
          "thinking": true,
          "web_search": true
        }
      },
      "write": {
        "enable_all_context_servers": true,
        "name": "Write",
        "tools": {
          "batch_tool": true,
          "code_actions": true,
          "code_symbols": true,
          "contents": true,
          "copy_path": false,
          "create_file": true,
          "delete_path": false,
          "diagnostics": true,
          "edit_file": true,
          "fetch": true,
          "find_path": true,
          "grep": true,
          "list_directory": false,
          "move_path": false,
          "now": true,
          "read_file": true,
          "rename": true,
          "symbol_info": true,
          "terminal": true,
          "thinking": true,
          "web_search": true
        }
      }
    }
  },
  "agent_font_size": 15.0,
  "auto_install_extensions": {
    "dune-theme": true,
    "elixir": true,
    "fish": true,
    "html": true,
    "yue-theme": true
  },
  "buffer_font_family": "Hermit",
  "buffer_font_size": 13.0,
  "collaboration_panel": {
    "button": false
  },
  "context_servers": {
    "tidewave": {
      "args": ["http://localhost:4000/tidewave/mcp"],
      "command": "/Users/petar/.local/bin/mcp-proxy",
      "env": null,
      "source": "custom"
    }
  },
  "current_line_highlight": "gutter",
  "cursor_blink": false,
  "edit_predictions": {
    "disabled_globs": [".env", "**/journal"],
    "mode": "subtle"
  },
  "experimental.theme_overrides": {
    "players": [
      {
        "selection": "#99FFE43d"
      }
    ]
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "file_types": {
    "CSS": ["scss"],
    "Shell Script": [".envrc", ".env"]
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "icon_theme": "Zed (Default)",
  "inlay_hints": {
    "enabled": false
  },
  "journal": {
    "hour_format": "hour24",
    "path": "~/Notes"
  },
  "language_models": {
    "lmstudio": {
      "available_models": [
        {
          "display_name": "qwen3-coder-30b",
          "max_tokens": 128000,
          "name": "qwen/qwen3-coder-30b",
          "supports_images": false,
          "supports_tool_calls": true
        }
      ]
    }
  },
  "languages": {
    "Elixir": {
      "language_servers": [
        "expert",
        "!elixir-ls",
        "!next-ls",
        "!lexical",
        "tailwindcss-language-server"
      ]
    },
    "HTML": {
      "hard_tabs": false,
      "tab_size": 2
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      },
      "hard_tabs": false,
      "tab_size": 2
    },
    "Markdown": {
      "show_edit_predictions": false
    },
    "Svelte": {
      "hard_tabs": false,
      "tab_size": 2
    },
    "TypeScript": {
      "hard_tabs": false,
      "tab_size": 2
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
          "enable": false
        }
      }
    },
    "elixir-ls": {
      "binary": {
        "path": "/Users/petar/.local/share/elixir-ls/language_server.sh"
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy"
        }
      }
    }
  },
  "middle_click_paste": true,
  "preferred_line_length": 100,
  "project_panel": {
    "dock": "left"
  },
  "relative_line_numbers": true,
  "scrollbar": {
    "show": "never"
  },
  "show_completions_on_input": true,
  "show_edit_predictions": true,
  "soft_wrap": "editor_width",
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "terminal": {
    "blinking": "off",
    "copy_on_select": true,
    "dock": "bottom",
    "env": {
      "TERM_PROGRAM": "zed"
    },
    "font_family": "Hermit",
    "font_size": 13.0,
    "option_as_meta": true,
    "shell": {
      "with_arguments": {
        "args": ["-l", "-i"],
        "program": "/opt/homebrew/bin/fish"
      }
    },
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "theme": {
    "dark": "Yue Newmoon",
    "light": "Gruvbox Light Hard",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "ui_font_family": "iA Writer Mono S",
  "ui_font_size": 13.0,
  "use_autoclose": true,
  "vertical_scroll_margin": 5,
  "vim": {
    "toggle_relative_line_numbers": true
  },
  "vim_mode": true
}
