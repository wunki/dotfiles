[
  {
    "context": "Editor && edit_prediction_conflict && showing_completions",
    "bindings": {
      "tab": "editor::AcceptEditPrediction"
    }
  },
  {
    "context": "Editor && (vim_mode == normal || vim_mode == visual) && !VimWaiting && !menu",
    "bindings": {
      "space f": "file_finder::Toggle",
      "space p n": "editor::GoToDiagnostic",
      "space p p": "editor::GoToPreviousDiagnostic",
      "space r n": "editor::Rename",
      "space r r": "editor::RestartLanguageServer",
      "space t i": "editor::ToggleInlayHints",
      "space t l": "editor::ToggleLineNumbers"
    }
  },
  {
    "context": "Editor && showing_completion",
    "bindings": {
      "ctrl-y": "editor::ConfirmCompletion"
    }
  },
  {
    "context": "Editor && VimControl && !VimWaiting && !menu",
    "bindings": {
      "space o": "tab_switcher::Toggle"
    }
  },
  {
    "context": "Editor && VimControl && !VimWaiting && !menu",
    "bindings": {
      "space h": "pane::TogglePinTab"
    }
  }
]
